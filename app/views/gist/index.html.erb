<h1>Listing Bookmarks</h1>
 
<table class="table table-striped">
	<thead>
		<tr>
		  <th>Cr√©ateur</th>
		  <th>Lrl</th>
		  <th>Languages</th>
		</tr>
	</thead>
    <tbody>
    	<% @bookmarks.each do |bookmark| %>
    	<tr>
		  <td><%= bookmark.user.name %></td>
		  <td><%= bookmark.url %></td>
		  <td>
		  	<%= bookmark.languages.each do |language| %>
		  	<%= language %>,
		  </td>
		</tr>
		<% end %>
    </tbody>
</table>
